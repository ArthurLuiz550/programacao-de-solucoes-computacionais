package meuPrimeiroProjetoJavaNoEclipse;
import javax.swing.JOptionPane;

public class MinhaPrimeiraClasse {
	public static void main(String args[]) {
		int num1 = 3, num2 = 5;
		System.out.println("num1 = " + num1);
		System.out.println("num2 = " + num2);
		System.out.printf("(double) num1 / num2 = %.2f \n", ((double) num1 / num2));
		System.out.printf("num1 / (double) num2 = %.3f \n", (num1 / (double) num2));
		System.out.printf("(double) num1 / (double) num2 = %.4f \n", ((double) num1 /
		(double) num2));
		
}
}